{
  "name": "Moovi H5P-video Supervisor by valonso",
  "version": "4.0",
  "description": "Extensi√≥n para poder manipular y obtener las respuestas de los videos interactivos H5P de moovi.uvigo.gal.",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/eye_large_logo.png",
      "32": "images/eye_large_logo.png",
      "48": "images/eye_large_logo.png",
      "128": "images/eye_large_logo.png"
    }
  },
  "icons": {
    "16": "images/eye_large_logo.png",
    "32": "images/eye_large_logo.png",
    "48": "images/eye_large_logo.png",
    "128": "images/eye_large_logo.png"
  },
  "permissions": ["activeTab"],
  "background": {
    "scripts": ["popup.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*.uvigo.gal/*"],
      "js": ["jquery.js", "moovi-video-content-script.js"]
    }
  ],
  "manifest_version": 2
}
